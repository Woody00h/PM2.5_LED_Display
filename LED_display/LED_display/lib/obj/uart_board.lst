   817                                                                                     ; Mask only errors and data ready
   818  2F22  21 E8                and  A,(UART_Board_RX_ERROR|UART_Board_RX_REG_FULL)
   819  2F24  08                   push A
   820  2F25  21 08                and  A,UART_Board_RX_COMPLETE                           ; Check if a character is ready
   821  2F27  B007                 jnz  .RX_GET_DATA                                       ; Data Ready go read it.
   822  2F29  18                   pop  A
   823  2F2A  29 01                or   A,UART_Board_RX_NO_DATA                            ; Add no data flag
   824  2F2C  4B                   swap A,X
   825  2F2D  8007                 jmp  End_UART_Board_iReadChar
   826                          
   827  2F2F                    .RX_GET_DATA:
   828  2F2F  18                   pop  A
   829  2F30  21 E0                and  A,UART_Board_RX_ERROR
   830  2F32  4B                   swap A,X
   831  2F33  5D 3E                mov  A,REG[UART_Board_RX_BUFFER_REG ]                   ; Read data first, then
   832                                                                                     ; determine if data is valid
   833                          
   834  2F35                     End_UART_Board_iReadChar:
   835                             RAM_EPILOGUE RAM_USE_CLASS_1
   836  2F35  7F                   ret
   837                          .ENDSECTION
