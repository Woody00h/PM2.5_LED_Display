   769  2F1B  4B                   swap A,X                                                ; Put data in A and exit
   770                             RAM_EPILOGUE RAM_USE_CLASS_1
   771  2F1C  7F                   ret
   772                          
   773  2F1D                    .RX_NO_VALID_CHAR:
   774  2F1D  50 00                mov A,0x00                                              ; Zero out character
   775                          
   776  2F1F                     End_UART_Board_cReadChar:
   777                             RAM_EPILOGUE RAM_USE_CLASS_1
   778  2F1F  7F                   ret
