   824  307C  4B                   swap A,X
   825  307D  8007                 jmp  End_UART_Sensor_iReadChar
   826                          
   827  307F                    .RX_GET_DATA:
   828  307F  18                   pop  A
   829  3080  21 E0                and  A,UART_Sensor_RX_ERROR
   830  3082  4B                   swap A,X
