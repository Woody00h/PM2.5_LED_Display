   753  2F09                    _UART_Board_cReadChar:
   754                             RAM_PROLOGUE RAM_USE_CLASS_1
   755  2F09  5D 3F                mov  A,REG[UART_Board_RX_CONTROL_REG]                   ; Get Status of RX
   756  2F0B  08                   push A
   757  2F0C  21 08                and  A,UART_Board_RX_COMPLETE                           ; Check if a character is ready
   758  2F0E  B004                 jnz  .RX_DATA_RDY                                       ; Data Ready go read it.
   759  2F10  18                   pop  A
   760  2F11  800B                 jmp  .RX_NO_VALID_CHAR
