   114                          ; LoadConfig - Set IO registers as specified in ROM table of (address,value)
   115                          ;              pairs. Terminate on address=0xFF.
   116                          ;
   117                          ;  INPUTS:  [A,X] points to the table to be loaded
   118                          ;           Flag Register Carry bit encodes the Register Bank
   119                          ;           (Carry=0 => Bank 0; Carry=1 => Bank 1)
   120                          ;
   121                          ;  RETURNS: nothing.
   122                          ;
   123                          ;  STACK FRAME:  X-4 I/O Bank 0/1 indicator
   124                          ;                X-3 Temporary store for register address
   125                          ;                X-2 LSB of config table address
   126                          ;                X-1 MSB of config table address
   127                          ;
   128  2B87                    LoadConfig:
   129  2B87  71 40                 RAM_PROLOGUE RAM_USE_CLASS_2
   130  2B89  38 02                 add     SP, 2                   ; Set up local vars
   131  2B8B  10                    push    X                       ; Save config table address on stack
   132  2B8C  08                    push    A
   133  2B8D  4F                    mov     X, SP
   134  2B8E  56 FC 00              mov     [X-4], 0                ; Set default Destination to Bank 0
   135  2B91  D004                  jnc     .BankSelectSaved        ; Carry says Bank 0 is OK
   136  2B93  56 FC 01              mov     [X-4], 1                ; No Carry: default to Bank 1
   137  2B96                    .BankSelectSaved:
   138  2B96  18                    pop     A
   139  2B97  20                    pop     X
   140                          
   141  2B98                    LoadConfigLp:
   142  2B98  70 EF                 M8C_SetBank0                    ; Switch to bank 0
   143  2B9A  62 E3 00              M8C_ClearWDT                    ; Clear the watchdog for long inits
   144  2B9D  10                    push    X                       ; Preserve the config table address
   145  2B9E  08                    push    A
   146  2B9F  28                    romx                            ; Load register address from table
   147  2BA0  39 FF                 cmp     A, END_CONFIG_TABLE     ; End of table?
   148  2BA2  A01F                  jz      EndLoadConfig           ;   Yes, go wrap it up
   149  2BA4  4F                    mov     X, SP                   ;
   150  2BA5  48 FC 01              tst     [X-4], 1                ; Loading IO Bank 1?
   151  2BA8  A003                  jz      .IOBankNowSet           ;    No, Bank 0 is fine
   152  2BAA  71 10                 M8C_SetBank1                    ;   Yes, switch to Bank 1
   153  2BAC                    .IOBankNowSet:
   154  2BAC  54 FD                 mov     [X-3], A                ; Stash the register address
   155  2BAE  18                    pop     A                       ; Retrieve the table address
   156  2BAF  20                    pop     X
   157  2BB0  75                    inc     X                       ; Advance to the data byte
   158  2BB1  09 00                 adc     A, 0
   159  2BB3  10                    push    X                       ; Save the config table address again
   160  2BB4  08                    push    A
   161  2BB5  28                    romx                            ; load config data from the table
   162  2BB6  4F                    mov     X, SP                   ; retrieve the register address
   163  2BB7  59 FD                 mov     X, [X-3]
   164  2BB9  61 00                 mov     reg[X], A               ; Configure the register
   165  2BBB  18                    pop     A                       ; retrieve the table address
   166  2BBC  20                    pop     X
   167  2BBD  75                    inc     X                       ; advance to next table entry
   168  2BBE  09 00                 adc     A, 0
   169  2BC0  8FD7                  jmp     LoadConfigLp            ; loop to configure another register
   170  2BC2                    EndLoadConfig:
   171  2BC2  38 FC                 add     SP, -4
